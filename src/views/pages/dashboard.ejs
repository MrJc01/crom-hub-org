<% /* Dynamic Layout Engine - Loops through sections_order from config */ %> <%
sections_order.forEach(function(sectionId) { %> <% const section =
sections_data[sectionId]; %> <% if (section && section.enabled) { %> <%-
include('../partials/sections/' + sectionId, { settings: section || {}, data,
organization, modules }) %> <% } %> <% }); %>
