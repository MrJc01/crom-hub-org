<% /* Dynamic Layout Engine - Loops through sections from config */ %> <%
sections.forEach(function(section) { %> <% if (section.enabled) { %> <% try { %>
<%- include('./partials/sections/' + section.id, { settings: section.settings ||
{}, data, organization, modules }) %> <% } catch(e) { %>
<!-- Section <%= section.id %> not found -->
<% } %> <% } %> <% }); %>
